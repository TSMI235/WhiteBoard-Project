<h3>Edit Whiteboard</h3>

<%= form_for @whiteboard do |f| %>
    <div>
        <%= f.label :title %>
        <%= f.text_field :title %>
    </div>

    <div>
        <%= f.label :description %>
        <%= f.text_field :description %>
    </div>
    
    <div>
        <%= f.submit %>
    </div>
<% end %>